@charset "utf-8";
/* global */
body {
	width: 100%;
	height: 100%;
	position: relative;
	font: normal 100% 'Microsoft YaHei', Helvetica, Arial, sans-serif;
	text-align: center;
	background: #fff;
}
button {
	font-family: 'Microsoft YaHei';
}
a {
	text-decoration: none;
	color: #000;
}
/* header */
header {
	position: fixed;
	width: 100%;
	top: 0;
	left: 0;
	z-index: 100;
}
#header {
	position: fixed;
	width: 100%;
	height: 2.4rem;
	top: 0;
	left: 0;
	z-index: 1;
	background: #000;
	box-shadow: 0 0 10px #ccc;
	-webkit-transition: all 5s ease;
	transition: all 5s ease;
}
#logo {
	display: inline-block;
	margin: 5px 30px;
}
header img {
	float: left;
	width: 1.5rem;
	height: 1.7rem;
	border-radius: 10px;
	-webkit-transition: box-shadow 0.5s;
	transition: box-shadow 0.5s;
	position: absolute;
	left: 50%;
	margin-left: -250px;
}
header img:hover {
	box-shadow: 0 0 15px #fff;
}
nav {
	text-align: center;
	display: inline-block;
	position: absolute;
	float: left;
	left: 50%;
	margin-left: -200px;
	margin-top: 18px;
}
nav li {
	float: left;
	position: relative;
	text-align: center;
	width: 100px;
	margin: -10px 0px 10px 0px;
}
nav li a {
	padding: 10px 0;
	position: relative;
	color: #fff;
	font-size: 15px;
	opacity: 1;
}
nav li a:after {
	content: "";
	position: absolute;
	left: 50%;
	right: 50%;
	bottom: 0;
	height: 2px;
	background: #fff;
	opacity: 0;
	-webkit-transition: .2s ease-in;
	transition: .2s ease-in;
}
nav li a:hover {
	color: #ddd;
}
nav li a:hover:after {
	left: 0;
	right: 0;
	opacity: 1;
}
 @media screen and (max-width:1366px) {
#logo {
	margin: 3px 30px 0 30px;
}
#header {
	height:35px;
}
nav {
	margin-top: 14px;
}
}
/*------section 1--------*/
#img1 img {
	opacity: 0;
	height: 100%;
	width: 100%;
	position: absolute;
	left: 0%
}
#img2 img {
	opacity: 0;
	position: absolute;
	height: 90%;
	left: 3%;
}
.section1, .section2 {
	background: #fff;
	background-size: 100%;
	opacity: 1;
}
#section_p1 {
	position: absolute;
	bottom: 53%;
	left: -120%;
	font-size: 3.5rem;
	color: #fff;
}
#section_p1 span, #section_p2 span {
	color: #C66;
}
#section_p2 {
	position: absolute;
	bottom: 44%;
	left: -120%;
	font-size: 2.5rem;
	color: #fff;
}
#section_p3 {
	position: absolute;
	bottom: 32%;
	left: -120%;
	font-size: 1.5rem;
	color: #fff;
	cursor:pointer;
	padding-bottom:7px;
}
#section_p3:after{
	content: "";
	position: absolute;
	left: 50%;
	right: 50%;
	bottom: 0;
	height: 4px;
	background: #C66;
	opacity: 0;
	-webkit-transition: .2s ease-out;
	transition: .2s ease-out;
}

#section_p3:hover:after {
	left: 0;
	right: 0;
	opacity: 1;
}
@media screen and (max-width:1366px){
.hidden_box{
	position:absolute;
	z-index:100;
	top:68%;
	left:15%;
	padding-bottom:15px;
	width:96px;
	display:none;
	background: top center;
	filter:alpha(opacity=100);
	-moz-opacity:1;
	opacity:1;
	font-size:18px;
	color:#fff;
}
.hidden_box>ul li{ 
	line-height:30px; 
	height:50px; 
	cursor:pointer; 
	}
}
/*------section 2--------*/
#property_list1 {
	position: absolute;
	right: -120%;
	top: 12%;
}
#property_list2 {
	position: absolute;
	right: -120%;
	top: 53%;
}
#property_pro1, #property_cou1 {
	cursor: pointer;
	webkit-transform: scale(1.0);
	transform: scale(1.0);
	transition : transform .25s;
}
#property_ox1, #property_co1 {
	font-size: 1.2rem;
	color: #AAA;
	line-height: 2;
}
.property_pro img, .property_cou img {
	width: 7rem;
	height: 7rem;
	margin-bottom: 1rem;
}
#property_cou1:hover {
	webkit-transform: scale(1.2);
	transform: scale(1.3);
	transition : transform .25s;
}
#property_pro1:hover {
	webkit-transform: scale(1.2);
	transform: scale(1.3);
	transition : transform .25s;
}
#property_p1, #property_p2 {
	font-size: 1.1rem;
	color: #000;
	opacity: 0;
	line-height: 2;
	margin-top: 30px;
}
#property_ox1 span {
	font-size: 1.8rem;
	color: #FC0;
}
#property_co1 span {
	font-size: 1.8rem;
	color: #33F;
}
@media screen and (max-width:1366px) {
#property_list1 {
	position: absolute;
	top: 10%;
}
#property_ox1, #property_co1 {
	font-size: 1.2rem;
	color: #AAA;
	line-height: 2;
}
.property_pro img, .property_cou img {
	width: 5rem;
	height: 5rem;
}
#property_p1, #property_p2 {
	font-size: 0.8rem;
	color: #000;
	opacity: 0;
	line-height: 2;
	margin-top: 10px;
}
#property_ox1 span {
	font-size: 1.8rem;
	color: #FC0;
}
#property_co1 span {
	font-size: 1.8rem;
	color: #33F;
}
}
